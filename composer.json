{
    "name": "bveldhuis/picasagallery",
    "description": "A simple Picasa(web) images gallery.",
    "type": "bolt-extension",
    "minimum-stability": "dev",
    "require": {
        "bolt/bolt": ">=3.0.0,<4.0.0"
    },
    "keywords" : [
        "Picasa",
		"images",
        "image",
		"photos",
        "gallery"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "BVeldhuis"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Bveldhuis\\PicasaGallery\\" : "src"
        }
    },
    
    "extra": {
        "bolt-class": "Bolt\\Extension\\Bveldhuis\\PicasaGallery\\PicasaGalleryExtension",
		"bolt-icon": "bolt-icon.png"
    }
}
